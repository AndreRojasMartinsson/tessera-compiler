export function d $std.math.addf(d %a, d %b) {
@start
  %result =d add %a, %b
  ret %result
}

export function w $main() {
@start
  %a =w add 5, 8
  %c =w call $printf(l $fmt, w %a)
  %d =d call $std.math.addf(d d_7.5, d d_7.5)
  %e =w call $printf(l $fmt_2, d %d)
  ret 0
}

data $fmt = { b "5 + 8 = %d\n" , b 0 }
data $fmt_2 = { b "7.5 + 7.5 = %f\n" , b 0 }
